{
  "publisher": "aryanpingle",
  "name": "dependograph",
  "displayName": "Dependograph",
  "description": "Get interactive visualizations of your project dependencies.",
  "version": "0.0.11",
  "icon": "./assets/images/extension-logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/aryanpingle/Dependograph.git"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "browser": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dependograph-sidebar",
          "title": "Dependograph",
          "icon": "assets/images/activity-bar-icon.svg"
        }
      ]
    },
    "views": {
      "dependograph-sidebar": [
        {
          "id": "dependograph-fs-explorer",
          "name": "Entry Files",
          "type": "tree"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "dependograph.refreshFSExplorer",
          "group": "navigation@1",
          "when": "view == dependograph-fs-explorer"
        },
        {
          "command": "dependograph.openVisualization",
          "group": "navigation@2",
          "when": "view == dependograph-fs-explorer"
        }
      ],
      "view/item/context": [
        {
          "command": "dependograph.openFile",
          "group": "inline@1",
          "when": "view == 'dependograph-fs-explorer' && viewItem != 'folder'"
        },
        {
          "command": "dependograph.chooseEntryFile",
          "group": "inline@2",
          "when": "view == 'dependograph-fs-explorer' && viewItem == 'file'"
        },
        {
          "command": "dependograph.dropEntryFile",
          "group": "inline@2",
          "when": "view == 'dependograph-fs-explorer' && viewItem == 'file--entry'"
        }
      ]
    },
    "commands": [
      {
        "command": "dependograph.chooseEntryFile",
        "title": "Add to entry files",
        "icon": "$(add)"
      },
      {
        "command": "dependograph.dropEntryFile",
        "title": "Remove from entry files",
        "icon": "$(remove)"
      },
      {
        "command": "dependograph.refreshFSExplorer",
        "icon": "$(refresh)",
        "title": "Refresh"
      },
      {
        "command": "dependograph.openVisualization",
        "icon": "$(window)",
        "title": "Create Visualization"
      },
      {
        "command": "dependograph.openFile",
        "icon": "$(go-to-file)",
        "title": "Open File"
      }
    ]
  },
  "scripts": {
    "clean": "rm -rf out/*",
    "compile": "npm run check-types && node esbuild.js --production",
    "check-types": "tsc --noEmit",
    "watch": "tsc --watch -p ./",
    "watch:esbuild": "node esbuild.js --production --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "vscode:prepublish": "npm run compile"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.6",
    "@types/d3": "^7.4.3",
    "@types/ejs": "^3.1.5",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "esbuild": "^0.21.5",
    "eslint": "^8.57.0",
    "git-format-staged": "^3.1.1",
    "husky": "^9.0.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@babel/generator": "7.18.2",
    "@babel/parser": "7.18.3",
    "@babel/traverse": "7.18.2",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "cli-table3": "0.6.2",
    "d3": "7.9.0",
    "ejs": "^3.1.10",
    "enhanced-resolve": "4.5.0",
    "enquirer": "2.3.6",
    "jsconfig-paths-webpack-plugin": "0.1.3",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "uuid": "^10.0.0",
    "vscode-webview-variables": "^1.0.2",
    "yargs": "17.5.1"
  }
}
